{
  "homepage": "https://daggerok.github.io/nuxt-examples/",
  "name": "simple-nuxt-examples",
  "version": "1.0.0",
  "description": "Simple Nuxt.js project",
  "author": "Maksim Kostromin",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "gh": "cross-env BASE_HREF='/nuxt-examples/' npm run generate",
    "predeploy": "npm-run-all predeploy:*",
    "predeploy:gh": "npm run gh",
    "deploy": "gh-pages -d ./dist -b gh-pages -m $(date +%Y-%m-%d):\\ GitHub\\ pages\\ deployment."
  },
  "dependencies": {
    "nuxt": "^2.0.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0"
  },
  "devDependencies": {
    "@nuxt/types": "0.5.3",
    "@types/node": "12.12.14",
    "cross-env": "6.0.3",
    "gh-pages": "2.1.1",
    "ncp": "2.0.0",
    "npm-run-all": "4.1.5"
  }
}
